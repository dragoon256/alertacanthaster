generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/acanthaster.db"
}

model Sighting {
  id        Int      @id @default(autoincrement())
  latitude  Float
  longitude Float
  count     Int
  certainty Int
  createdAt DateTime @default(now())
}
